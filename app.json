{
    "name": "EmiliaX Music",
    "logo": "https://telegra.ph/file/07bd09306a4485583893e.jpg",
    "description": "EmiliaX Music allow you to stream music trought the telegram voice chat feature.",
    "keywords": ["music", "voice chat", "telegram"],
    "repository": "https://github.com/EmiliaTzy/EmiliaXMusic",
    "stack": "container",
    "env": {
        "SESSION_NAME": {
            "description": "isi dengan string Pyrogram (P)",
            "required": true
        },
        "ARQ_API_KEY": {
            "description": "Untuk lirik dan quotly, dapatkan di @ARQRobot",
            "required": true
        },
        "BOT_TOKEN": {
            "description": "Token botmu dari @BotFather",
            "required": true
        },
        "BOT_NAME": {
            "description": "Nama bot Musikmu.",
            "required": true,
            "value": ""
        },
        "BOT_USERNAME": {
            "description": "isi username bot tanpa @",
            "required": true,
            "value": "EmiliaMusic1_Bot"
        },
        "U_BRANCH": {
            "description": "Branch ini dari repo JANGAN DIUBAH!",
            "required": true,
            "value": "main"
        },
        "HEROKU_API_KEY": {
            "description": "heroku api key dapatkan di pengaturan akun heroku.",
            "required": true,
            "value": ""
        },
        "HEROKU_APP_NAME": {
            "description": "heroku app name, dapatkan dari nama paling atas",
            "required": true,
            "value": ""
        },
        "UPSTREAM_REPO": {
            "description": "Jika kamu fork, taruh link forkmu disini. namun jika tidak ABAIKAN SAJA!",
            "required": true,
            "value": "https://github.com/EmiliaTzy/EmiliaXMusic"
        },
        "BOT_IMG": {
            "description": "Untuk alive pic.",
            "required": false,
            "value": "https://telegra.ph/file/5f5f8e412644c47cc836a.jpg"
        },
        "ASSISTANT_NAME": {
            "description": "Username Asistan tanpa @",
            "required": true,
            "value": "emiliaassistant"
        },
        "BG_IMAGE": {
            "description": "Untuk background gambar saat memutar",
            "required": false,
            "value": "https://telegra.ph/file/0f6f8a8a5ad69fe5ecf3d.png"
        },
        "ALIVE_EMOJI": {
            "description": "untuk emoji di perintah /alive ",
            "required": true,
            "value": "ðŸŒ»"
        },
        "OWNER_NAME": {
            "description": "Username akunmu tanpa @",
            "required": false,
            "value": "azkiya"
        },
        "API_ID": {
            "description": "APP ID dari my.telegram.org",
            "required": true
        },
        "API_HASH": {
            "description": "API HASH dari my.telegram.org",
            "required": true
        },
        "SUDO_USERS": {
            "description": "Taruh ID untuk dijadikan admin untuk bot (dipisahkan with spasi).",
            "required": true
        },
        "DURATION_LIMIT": {
            "description": "Batas durasi untuk musik yang diputar (dalam menit).",
            "required": true,
            "value": "15"
        },
        "PMPERMIT": {
            "description": "Untuk PMPERMIT di akun asistan.",
            "required": false,
            "value": "ENABLE"
        },
        "UPDATES_CHANNEL": {
            "description": "Taruh Username channelmu tanpa @, Jika tidak punya BIARKAN SAJA",
            "required": false,
            "value": "EmiliaProject"
        },
        "GROUP_SUPPORT": {
            "description": "Taruh username Grupmu tanpa @, Jika tidak punya BIARKAN SAJA",
            "required": false,
            "value": "EmiliaSupport"
        }
    },
    "buildpacks": [
    {
      "url": "https://github.com/jonathanong/heroku-buildpack-ffmpeg-latest"
    },
    {
      "url": "heroku/python"
    }
  ]
}
